<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.112" fx:controller="gui.ModeWinController">
    <children>
        <VBox id="VBox" alignment="CENTER" layoutX="200.0" layoutY="153.0" spacing="28.0">
            <children>
                <Button fx:id="storyBtn" minWidth="180.0" mnemonicParsing="false" onAction="#onStoryBtnCLick"
                        text="Story">
                    <font>
                        <Font name="Bauhaus 93" size="18.0" fx:id="x1"/>
                    </font>
                </Button>
                <Button font="$x1" minWidth="180.0" mnemonicParsing="false" onAction="#onBirthdayBtnClick"
                        text="Birthday"/>
                <Button font="$x1" minWidth="180.0" mnemonicParsing="false" onAction="#onEndlessBtnCLick"
                        text="Endless"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
