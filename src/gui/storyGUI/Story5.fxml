<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="gui.storyGUI.Story5WinController">
    <children>
        <ImageView fx:id="picture1" fitHeight="800.0" fitWidth="1200.0">
            <image>
                <Image url="@../img/story5.jpg"/>
            </image>
        </ImageView>
        <ImageView fx:id="picture2" fitHeight="243.0" fitWidth="1084.0" layoutY="557.0">
            <image>
                <Image url="@../img/storyChart2.png"/>
            </image>
        </ImageView>
        <Button fx:id="skipBtn" layoutX="1115.0" layoutY="6.0" mnemonicParsing="false"
                onMouseClicked="#onSkipBtnClicked" prefHeight="40.0" prefWidth="80.0" stylesheets="@story.css"
                text="Skip" underline="true">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Separator fx:id="seperate" layoutX="-5.0" layoutY="581.0" prefHeight="5.0" prefWidth="1200.0"/>
        <ImageView fx:id="picture3" fitHeight="264.0" fitWidth="246.0" layoutX="949.0" layoutY="528.0">
            <image>
                <Image url="@../img/boy.png"/>
            </image>
        </ImageView>
        <Label fx:id="subline" layoutX="76.0" layoutY="628.0" onMouseClicked="#onChartClicked" prefHeight="119.0"
               prefWidth="667.0" text="Story begins..." textFill="#83337e">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
